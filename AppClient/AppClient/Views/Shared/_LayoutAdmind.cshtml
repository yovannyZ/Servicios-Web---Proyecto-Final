@{
    AppClient.CanchitaWS.Usuario usuario = (AppClient.CanchitaWS.Usuario)Session["usuario"];
    AppClient.CanchitaWS.Sede sede = (AppClient.CanchitaWS.Sede)Session["sede"];
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="msapplication-tap-highlight" content="no">
    <meta name="description" content="Materialize is a Material Design Admin Template,It's modern, responsive and based on Material Design by Google. ">
    <meta name="keywords" content="materialize, admin template, dashboard template, flat admin template, responsive admin template,">
    <title>Canchita la 10</title>

    <!-- Favicons-->

    <link rel="icon" href="~/content/images/favicon/favicon-32x32.png" sizes="32x32">
    <!-- Favicons-->

    <link rel="apple-touch-icon-precomposed" href="~/content/materialize/images/favicon/apple-touch-icon-152x152.png">
    <!-- For iPhone -->
    <meta name="msapplication-TileColor" content="#00bcd4">
    <meta name="msapplication-TileImage" content="images/favicon/mstile-144x144.png">
    <!-- For Windows Phone -->
    <!-- CORE CSS-->
    <link href="~/content/css/materialize.css" rel="stylesheet" media="screen,projection" />
    <link href="~/content/css/style.css" rel="stylesheet" media="screen,projection" />
    <link href="~/content/js/plugins/data-tables/css/jquery.dataTables.min.css" rel="stylesheet" media="screen,projection" />
   

  
   
    <!-- INCLUDED PLUGIN CSS ON THIS PAGE -->
    <link href="~/content/css/prism.css" rel="stylesheet" />
    <link href="~/content/js/plugins/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="screen,projection" />
   
    <link href="~/content/js/plugins/jvectormap/jquery-jvectormap.css" rel="stylesheet" media="screen,projection" />
   <!--<link href="~/content/js/plugins/chartist-js/chartist.min.css" rel="stylesheet" media="screen,projection" /> -->



</head>

<body>
    <!-- Start Page Loading -->
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!-- End Page Loading -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START HEADER -->
    <header id="header" class="page-topbar">
        <!-- start header nav-->
        <div class="navbar-fixed">
            <nav class="cyan">
                <div class="nav-wrapper">
                    <h1 class="logo-wrapper"><a href="index.html" class="brand-logo darken-1"><img src="~/content/images/canchita-logo.png" alt="materialize logo" /></a> <span class="logo-text">Materialize</span></h1>
                    <ul class="right hide-on-med-and-down">
                        <li class="search-out">
                            <input type="text" class="search-out-text">
                        </li>
                        <li>@sede.Descripcion</li>
                        <li>
                            <a href="/Admin/CerrarSesion" class="waves-effect waves-block waves-light show-search"><i class="mdi-content-clear"></i></a>
                        </li>
                        <li>
                            <a href="javascript:void(0);" class="waves-effect waves-block waves-light toggle-fullscreen"><i class="mdi-action-settings-overscan"></i></a>
                        </li>
                       
                       
                    </ul>
                </div>
            </nav>
        </div>
        <!-- end header nav-->
    </header>
    <!-- END HEADER -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START MAIN -->
    <div id="main">
        <!-- START WRAPPER -->
        <div class="wrapper">

            <!-- START LEFT SIDEBAR NAV-->
            <aside id="left-sidebar-nav">
                <ul id="slide-out" class="side-nav fixed leftside-navigation">
                    <li class="user-details cyan darken-2">
                        <div class="row">
                            <div class="col col s4 m4 l4">
                                @{ var imagen = string.Empty;
                                 if (usuario.Imagen != null)
                                 {
                                     var base64 = Convert.ToBase64String(usuario.Imagen);
                                     imagen = string.Format("data:image/jpg;base64,{0}", base64);
                                 }
                                }
                                <img src="@imagen" alt="" class="circle responsive-img valign profile-image">
                            </div>
                            <div class="col col s8 m8 l8">
                                <ul id="profile-dropdown" class="dropdown-content">
                                    <li>
                                    <a href="/Admin/CerrarSesion" ><i class="mdi-hardware-keyboard-tab"></i>Salir</a>
                                    </li>
                                </ul>
                                <a class="btn-flat dropdown-button waves-effect waves-light white-text profile-btn" href="#" data-activates="profile-dropdown">@usuario.Nombres &nbsp; @usuario.Apellidos<i class="mdi-navigation-arrow-drop-down right"></i></a>
                                <p class="user-roal">Administrador</p>
                            </div>
                        </div>
                    </li>
                    <li class="bold">
                        <a href="/Sede/" class="waves-effect waves-cyan"><i class="mdi-action-language"></i> Gestionar Sedes</a>
                    </li>
                    <li>
                        <hr />
                    </li>
                    <li class="bold">
                        <a href="/Campo/" class="waves-effect waves-cyan"><i class="mdi-communication-email"></i> Gestionar Campos</a>
                    </li>
                    <li>
                        <hr />
                    </li>
                    <li class="bold">
                        <a href="/Reserva/NuevaReserva" class="waves-effect waves-cyan"><i class="mdi-action-note-add"></i>Nueva Reserva</a>
                    </li>
                    <li>
                        <hr />
                    </li>
                    <li class="bold">
                        <a href="/Reserva/ListadoReserva" class="waves-effect waves-cyan"><i class="mdi-action-add-shopping-cart"></i>Ver Reservas</a>
                    </li>
                    <!--
                    <li class="bold">
                        <a href="/Reserva/ListadoReservaPendiente" class="waves-effect waves-cyan"><i class="mdi-action-event"></i>Ver Reservas Pendientes</a>
                    </li>
                        -->
                    <li>
                        <hr />
                    </li>
                    <li class="bold">
                        <a href="/Tarifa/" class="waves-effect waves-cyan"><i class="mdi-action-alarm-on"></i>Gestionar Tarifas</a>
                    </li>
                    <li>
                        <hr />
                    </li>
                    <li class="bold">
                        <a href="/Usuario/listaUsuario" class="waves-effect waves-cyan"><i class="mdi-action-account-child"></i>Gestionar Usuarios</a>
                    </li>
                    <li>
                        <hr />
                    </li>
                    <li class="bold">
                        <a href="/Report/Index" class="waves-effect waves-cyan"><i class="mdi-action-assessment"></i>Reporte</a>
                    </li>
                 
                    <li>
                        <hr />
                    </li>
                    <!--
                        <li class="bold">
                        <a href="/Reserva/Verificar" class="waves-effect waves-cyan"><i class="mdi-av-timer"></i>Verificar Reservas Pendientes</a>
                    </li>
                    -->
                    


                 
                </ul>
                <a href="#" data-activates="slide-out" class="sidebar-collapse btn-floating btn-medium waves-effect waves-light hide-on-large-only darken-2"><i class="mdi-navigation-menu"></i></a>
            </aside>
            <!-- END LEFT SIDEBAR NAV-->
            <!-- //////////////////////////////////////////////////////////////////////////// -->
            <!-- START CONTENT -->
            <section id="content">

                <!--start container-->
                <div class="container grey lighten-3" >
                    @RenderBody()
                </div>
                <!--end container-->
            </section>
            <!-- END CONTENT -->
            <!-- //////////////////////////////////////////////////////////////////////////// -->
            
            <!-- LEFT RIGHT SIDEBAR NAV-->

        </div>
        <!-- END WRAPPER -->

    </div>
    <!-- END MAIN -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- START FOOTER -->
    <footer class="page-footer">
        <div class="container">

        </div>
        <div class="footer-copyright">

        </div>
    </footer>
    <!-- END FOOTER -->
    <!-- ================================================
    Scripts
    ================================================ -->
    <!-- jQuery Library -->
    <script src="~/content/js/jquery-1.11.2.min.js"></script>

    <!--materialize js-->
    <script src="~/content/js/materialize.min.js"></script>

    <!--prism-->
    <script src="~/content/js/prism.js"></script>


    <!--scrollbar-->
    <script src="~/content/js/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>

    <!-- data-tables -->
    <script src="~/content/js/plugins/data-tables/js/jquery.dataTables.min.js"></script>

    <script src="~/content/js/plugins/data-tables/data-tables-script.js"></script>


    <!-- chartist -->
    <script src="~/content/js/plugins/chartist-js/chartist.min.js"></script>

    <!-- chartjs
    <script src="~/content/js/plugins/chartjs/chart.min.js"></script>
    <script src="~/content/js/plugins/chartjs/chart-script.js"></script> -->

    <!-- sparkline -->
    <script src="~/content/js/plugins/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/content/js/plugins/sparkline/sparkline-script.js"></script>


    <!--jvectormap-->
    <script src="~/content/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/content/js/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/content/js/plugins/jvectormap/vectormap-script.js"></script>

    <script src="~/content/js/jquery.unobtrusive-ajax.js"></script>

    @RenderSection("scripts",false)


    <!--plugins.js - Some Specific JS codes for Plugin Settings-->
    <script src="~/content/js/plugins.js"></script>
    <!-- Toast Notification -->
    <script type="text/javascript">
        // Toast Notification
        $(document).ready(function () {
            $('select').material_select();
            $('.slider').slider({ full_width: false });
        });
       

    </script>
</body>

</html>